@import url("calendar.css");
/* CSS Document */

body {
	margin-top: 10px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
	background: #FFFFFF;
}
body.popup {
	margin-top: 5px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
	background: #7dbdeb;
}
table {
	width:100%;
	
}

.table_width{
	width:0%;
	font-family: Tahoma, Helvetica, sans-serif;
	font-size: 11px;
	font-weight:normal;
	color: #0b333d;
}

/*Top Banner class*/
.logo {
	background-image:url("../images/SBILife_Logo.jpg");
	background-repeat: no-repeat;
	background-position:3px top;
	height: 56px;
	text-align:right;
	vertical-align:bottom;
	padding-right:10px;
	padding-bottom:4px;
	background-color:#ffffff;
}


/*Info class*/
.info {
	font-family: Tahoma, Helvetica, sans-serif;
	font-size: 11px;
	color: #000000;
	font-weight:normal;
	white-space:nowrap;
	padding-right:0px;
	padding-left:8px;
	padding-top:3px;
	padding-bottom:3px;
	
	}
.info_welcome {
	/*background-image:url("../images/welcome.gif");
	background-repeat: no-repeat;
	background-position:left 2px;*/
	font-family: Tahoma, Helvetica, sans-serif;
	font-size: 11px;
	color: #000000;
	font-weight:normal;
	vertical-align:bottom;
	white-space:nowrap;
	padding-right:0px;
	padding-left:23px;
	padding-top:2px;
	padding-bottom:3px;
	}
.info_logout {
	background-image:url("../images/logout.gif");
	background-repeat: no-repeat;
	background-position:7px 10px;
	font-family: Verdana, Helvetica, sans-serif;
	font-size: 11px;
	color: #f22a00;
	text-decoration:normal;
	font-weight:normal;
	vertical-align:bottom;
	white-space:nowrap;
	padding-right:0px;
	padding-left:32px;
	padding-top:2px;
	padding-bottom:3px;
	cursor:pointer;
	}
.info_logoutOver {
	background-image:url("../images/logout.gif");
	background-repeat: no-repeat;
	background-position:7px 10px;
	font-family: Verdana, Helvetica, sans-serif;
	font-size: 11px;
	color: #f22a00;
	text-decoration:underline;
	font-weight:normal;
	vertical-align:bottom;
	white-space:nowrap;
	padding-right:0px;
	padding-left:32px;
	padding-top:2px;
	padding-bottom:3px;
	cursor:pointer;
	}
	
.info_data {
	font-family: arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #0292c9;
	font-weight:bold;
	border-right:1px solid #03769c;
	white-space:nowrap;
	padding-right:6px;
	padding-left:7px;
	padding-top:2px;
	padding-bottom:2px;
	
	}
	.close {
	background-image:url("../images/close-image.gif");
	background-repeat: no-repeat;
	background-position:center center;
	font-family: Verdana, Helvetica, sans-serif;
	font-size: 11px;
	color: #f22a00;
	text-decoration:normal;
	font-weight:normal;
	vertical-align:bottom;
	white-space:nowrap;
	padding-right:0px;
	padding-left:50px;
	padding-top:2px;
	padding-bottom:3px;
	cursor:pointer;
	}
.closeOver {
	background-image:url("../images/close-image_over.gif");
	background-repeat: no-repeat;
	background-position:center center;
	font-family: Verdana, Helvetica, sans-serif;
	font-size: 11px;
	color: #f22a00;
	text-decoration:underline;
	font-weight:normal;
	vertical-align:bottom;
	white-space:nowrap;
	padding-right:0px;
	padding-left:52px;
	padding-top:2px;
	padding-bottom:3px;
	cursor:pointer;
	}
	
/*top Navigation*/
.top_nav_bg{
	background-image:url(../images/top_nav_bg.gif);
	background-repeat:repeat-x;
	background-position:left top;
	border-top:1px solid #b1dff6;
	border-bottom:1px solid #44a4d5;
	height:27px;
}
.top_tab{
	
  	height:25px;
	padding-left:13px;
	padding-right:13px;
	font-family: arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #050505;
	font-weight:bold;
	cursor:pointer;
	white-space:nowrap;
}
.top_tab_div{
	background-image:url(../images/top_tab.gif);
	background-repeat:no-repeat;
	background-position:center center;
  	height:25px;
	width:6px;
}
.top_tab_sel{
    background-image:url(../images/top_tab_sel.gif);
	background-repeat:repeat-x;
	background-position:top left;
	height:25px;
	padding-left:13px;
	padding-right:13px;
	font-family: arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #ffffff;
	font-weight:bold;
	white-space:nowrap;
}
.top_tab_over{
   background-image:url(../images/top_tab_over.gif);
	background-repeat:repeat-x;
	background-position:top left;
	background-color:#bbe7fc;
	border-top:1px solid #e5f4fc;
	height:25px;
	padding-left:13px;
	padding-right:13px;
	font-family: arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #050505;
	font-weight:bold;
	cursor:pointer;
	white-space:nowrap;
}
.div_bg {
	background-image:url(../images/div_bg.gif);
	background-repeat:repeat-x;
	background-position:top left;
	height:11px;
	border-top:1px solid #ffffff;
	background-color:#ffffff;
}





/*Container*/
.con {
	height:100%;
	vertical-align:top;
	background-image:url(../images/con_bg.gif);
	background-position:bottom left;
	background-repeat:repeat-x;
	border-bottom:1px solid #f9fbfd;
	background-color:#ffffff;

}
.con_table {
	
	height:100%;
	
}

.con_left {
	border-right:1px solid #c4c5c7;
	border-top:1px solid #c4c5c7;
	width:20%;
	vertical-align:top;
	padding-right:2px;
	
}
.con_right {
	vertical-align:top;
	padding-left:8px;
	padding-right:3px;
	padding-bottom:20px;
	border-left:2px solid #e6e6e6;
	
}
.con_mid {
	vertical-align:top;
	padding-left:5px;
	padding-right:3px;
	padding-bottom:15px;
}




/*Left Navigation */
.left_nav_table{
	
	border-bottom:2px solid #e6e6e6;

}
.left_tab_sel {
	background-image:url(../images/left_tab_sel.gif);
	background-repeat:repeat-x;
	background-position:left bottom;
	background-color:#ffffff;
	border-bottom:1px none #ffffff;
	height:27px;
	font-family: arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #020407;
	font-weight:bold;
	padding-left:12px;
	padding-top:8px;
	padding-right:0px;
	padding-bottom:5px;
	white-space:nowrap;
}
.left_tab_sel_img {
	background-image:url(../images/left_tab_sel_img.gif);
	background-repeat:no-repeat;
	background-position:right center;
	width:100%;
	white-space:nowrap;
}
.left_tab {
	background-image:url(../images/left_tab.gif);
	background-repeat:repeat-x;
	background-position:left bottom;
	background-color:#ffffff;
	border-top:1px solid #ffffff;
	border-bottom:1px solid #c4c5c7;
	height:25px;
	font-family: Tahoma, Helvetica, sans-serif;
	font-size: 11px;
	color: #000101;
	font-weight:normal;
	padding-left:12px;
	padding-top:6px;
	padding-right:0px;
	padding-bottom:5px;
	cursor:pointer;
	white-space:nowrap;
}
.left_tab_over {
	background-image:url(../images/left_tab_over.gif);
	background-repeat:repeat-x;
	background-position:left bottom;
	background-color:#ffffff;
	border-top:1px solid #ffffff;
	border-bottom:1px solid #c4c5c7;
	height:25px;
	font-family: Tahoma, Helvetica, sans-serif;
	font-size: 11px;
	color: #000101;
	font-weight:normal;
	padding-left:12px;
	padding-top:6px;
	padding-right:0px;
	padding-bottom:5px;
	cursor:pointer;
	white-space:nowrap;
}

/*Page*/

.page_title {
	font-family: tahoma,arial, "Times New Roman";
	font-size: 14px;
	color: #0174ae;
	background-color:#ffffff;
	font-weight:bold;
	height:28px;
	vertical-align:middle;
	padding-left:3px;
	background-image:url(../images/page_title.gif);
	background-position:left bottom;
	background-repeat:repeat-x;
	border-bottom:2px solid #ffffff;
}
.popup_title {
	
	background-image:url(../images/pop_up_title.gif);
	background-repeat:repeat-x;
	background-position:top left;
	border-top:3px solid #ffffff;
	background-color:#ffffff;
	font-family: arial, Helvetica, sans-serif;
